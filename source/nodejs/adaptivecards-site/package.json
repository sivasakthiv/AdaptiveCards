{
  "name": "adaptivecards-site",
  "version": "0.4.8",
  "private": true,
  "scripts": {
    "clean": "hexo clean",
    "build": "hexo generate",
    "generate-md-ac": "node node_modules/marked-schema/bin/marked-schema ../../../schemas/adaptive-card.json ./schema-explorer-toc.yml -r AdaptiveCard",
    "generate-md-hc": "node node_modules/marked-schema/bin/marked-schema ../../../schemas/host-config.json ./schema-hostconfig-toc.yml -r AdaptiveCardConfig",
    "release": "hexo clean && hexo generate",
    "start": "hexo server"
  },
  "hexo": {
    "version": "4.2.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "ac-typed-schema": "^0.5.6",
    "adaptivecards": "^2.0.0",
    "adaptivecards-designer": "^1.0.0",
    "adaptivecards-templating": "^1.0.0",
    "adaptivecards-visualizer": "^1.2.8",
    "change-case": "^3.0.1",
    "cheerio": "^1.0.0-rc.3",
    "glob": "^7.1.6",
    "hexo": "^4.2.1",
    "hexo-all-minifier": "^0.5.3",
    "hexo-browsersync": "^0.3.0",
    "hexo-cli": "^3.1.0",
    "hexo-featured-image": "^0.4.3",
    "hexo-fs": "^3.1.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^2.2.0",

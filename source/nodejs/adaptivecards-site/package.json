    "markdown-it": "^8.4.2",
    "marked-schema": "^0.1.4",
    "md5": "^2.2.1",
    "minimist": "^1.2.5",
    "monaco-editor": "^0.20.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
